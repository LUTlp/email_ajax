<h1>Email#index</h1>
<p>Find me in app/views/email/index.html.erb</p>

<div>
    <table>
      <thead>
        <tr>
          <th>Object</th>
          <th>Content</th>
        </tr>
      </thead>

      <tbody>
        <% @emails.each  do |s| %>
          <tr>
            <td>
              <a ><%= link_to s.object, email_path(s), remote: true %></a>
            </td>
            <td>
              <div id="content" style="display:none;">

              </div>
            </td>

          </tr>
        <% end %>
    </tbody>
    </table>
</div>
